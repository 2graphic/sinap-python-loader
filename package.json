{
  "name": "sinap-python-loader",
  "description": "Bindings for synap-types from python",
  "version": "0.0.4",
  "readme": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/2graphic/sinap-python-loader"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/!(test-)*"
  ],
  "scripts": {
    "format": "tsfmt -r && tslint --fix --project .",
    "lint": "tsfmt --verify && tslint --project .",
    "build": "tsc -p .",
    "test:nobuild": "mocha --colors lib/test-*.js",
    "test": "npm run build && npm run test:nobuild",
    "prepublishOnly": "npm run lint && npm run test",
    "clean": "rimraf ./node_modules ./typings ./lib"
  },
  "devDependencies": {
    "@types/node": "^7.0.12",
    "chai": "^3.5.0",
    "@types/chai": "^3.4.35",
    "mocha": "^3.2.0",
    "@types/mocha": "^2.2.40",
    "rimraf": "^2.6.1",
    "tslint": "^4.5.1",
    "typescript-formatter": "^5.1.2",
    "typescript": "2.2.2"
  },
  "dependencies": {
    "sinap-core": "~3.11.3"
  }
}
